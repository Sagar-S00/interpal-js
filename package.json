{
  "name": "interpal-js",
  "version": "0.1.0",
  "description": "Node.js port of the Interpal Python client with sync/async APIs",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --sourcemap",
    "dev": "tsup src/index.ts --watch --format cjs,esm --dts --sourcemap",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\""
  },
  "keywords": [
    "interpals",
    "client",
    "websocket",
    "async",
    "bot"
  ],
  "engines": {
    "node": ">=18.18.0"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "axios-retry": "^4.5.0",
    "bottleneck": "^2.19.5",
    "lru-cache": "^11.0.0",
    "node-fetch": "^3.3.2",
    "undici": "^6.19.8",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/ws": "^8.5.10",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.4",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  }
}

